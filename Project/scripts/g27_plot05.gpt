#Random with Usual    
set  autoscale                        # scale axes automatically
unset log                              # remove any log-scaling
unset label                            # remove any previous labels
set xtic auto                          # set xtics automatically
set ytic auto                          # set ytics automatically
set format y "%g ms"
set terminal png 
set output '../plots/g27_plot05.png'
stats "plot_step.gnu" u 1:2 nooutput
set title "Initial and Random step times"
set xlabel "Iterations"
set ylabel "Step time"
init(x) = a1*x + b1
fit init(x) 'plot_step.gnu' u 1:2 via a1, b1

random(x)= a2*x + b2
fit random(x) 'plot_random.gnu' u 1:2 via a2, b2

plot "plot_step.gnu" using 1:2 title "Initial CSV",init(x) title sprintf('Initial Regression = %.2fx + %.2f', a1, b1),\
     "plot_random.gnu" using 1:2 title "Random CSV",random(x) title sprintf('Random Regression = %.2fx + %.2f', a2, b2)
