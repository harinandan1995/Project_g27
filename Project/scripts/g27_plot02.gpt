#All Variables
set  autoscale                        # scale axes automatically
unset log                              # remove any log-scaling
unset label                            # remove any previous labels
set xtic auto                          # set xtics automatically
set ytic auto                          # set ytics automatically
set format y "%g ms"
set terminal png
set title "Average time for all variables"
set ylabel "Average Time"
set xlabel "Iterations"
set output '../plots/g27_plot02.png'
#set yr [0:0.2]
plot "plot_allvars.gnu" using 1:2 title 'Step time' with line,\
     "plot_allvars.gnu" using 1:3 title 'Collision time' with line,\
     "plot_allvars.gnu" using 1:4 title 'Velocity' with line,\
     "plot_allvars.gnu" using 1:5 title 'Position' with line,\
     "plot_allvars.gnu" using 1:7 title 'Cumulative time' with line
